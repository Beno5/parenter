<div class="navbar navbar-expand-sm navbar-light custom_nav_color">
  <%= link_to root_path, class: "navbar-brand" do %>
    <%= image_tag "Parenter Logo 1.png"%>
  <% end %>
  <% if user_signed_in? %>
    <ul class="navbar-nav">
      <li class="nav-item active">
        <%= link_to "Home", root_path, class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "Messages", "#", class: "nav-link" %>
      </li>
      <li class>
        <%= link_to "Find Nanny", users_path, class: "nav-link" %>
      </li>
      <li class>
        <%= link_to "My Dashboard", dashboards_path, class: "nav-link" %>
      </li>
    </ul>
    <div class="nav-item dropdown">
      <%= cl_image_tag current_user.photo.key, class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
      <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
        <%= link_to "Action", "#", class: "dropdown-item" %>
        <%= link_to "Another action", "#", class: "dropdown-item" %>
        <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
      </div>
    </div>
  <% else %>
    <ul>
      <li class>
        <%= link_to "Home", root_path, class: "nav-link" %>
      </li>
      <li class>
        <%= link_to "Find Nanny", users_path, class: "nav-link" %>
      </li>
      <li class>
        <%= link_to "My Dashboard", dashboards_path, class: "nav-link" %>
      </li>
      <li class= "nav-item">
        <%= link_to "Login", new_user_session_path, class: "nav-link" %>
      </li>
    </ul>
  <% end %>
</div>
